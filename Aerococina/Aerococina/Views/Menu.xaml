<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="Aerococina.Views.Menu"
    xmlns:pages="clr-namespace:Aerococina.Views">
  <MasterDetailPage.Master>
    <pages:MenuMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage BarTextColor="White" BarBackgroundColor="#003454">
      <x:Arguments>
        <pages:MenuDetail />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>
